<?xml version="1.0" encoding="utf-8"?>
<me.goldze.mvvmhabit.view.shape.RadiusLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginStart="@dimen/d63"
    android:layout_marginEnd="@dimen/d63"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:padding="@dimen/d16"
    app:rv_backgroundColor="@color/white"
    app:rv_radius="@dimen/d5">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提示"
        android:textColor="@color/color_dark_333333"
        android:textSize="@dimen/font16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/d55"
        android:gravity="center"
        android:text="注册成功，请前往个人心完善个人资料"
        android:textColor="@color/color_dark_333333"
        android:textSize="@dimen/font16" />

    <me.goldze.mvvmhabit.view.shape.RadiusTextView
        android:id="@+id/tv_ppu_confirm"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d40"
        android:layout_marginTop="@dimen/d44"
        android:gravity="center"
        android:text="确定"
        android:textSize="@dimen/font16"
        app:rv_backgroundColor="@color/color_FF8C00"
        app:rv_radius="@dimen/d20"
        app:rv_textColor="@color/white" />

</me.goldze.mvvmhabit.view.shape.RadiusLinearLayout>